library(rpart)
library(rpart.plot)
data=read.csv("c:\\users\\ganesh\\onedrive\\desktop\\dwdm\\gender.csv")
tree <-rpart(height ~ gender+weight,data)
a <-data.frame(gender =c("male"),weight=c(85))
result <-predict(tree,a)
print(result)
rpart.plot(tree)

tree1 <-rpart(gender ~height+weight,data)
a <-data.frame(gender =c(170),weight=c(85))
result <-predict(tree,a)
print(result)
rpart.plot(tree)
